<table>
    <thead>
        <tr>
            <th> word </th>
            <th> counter </th>
            <th> frequencey </th>
            <th> syllable </th>
            <th> group </th>
            <th> rank one </th>
            <th> rank two </th>
            <th> predicate </th>
            
        </tr> 
    </thead>
    <tbody>

    <tbody>
        <% @subtitles.each do |subtitle| %>
            <tr>
                <td class="sub-word"><%= subtitle.word %></td>
                <td><%= subtitle.counter %></td>
                <td><%= subtitle.duration.to_f / subtitle.counter %><span class="mins"> mins</span></td>
                <td><%= subtitle.syllable %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<%= will_paginate @subtitles, inner_window: 1, outer_window: 1 %>
