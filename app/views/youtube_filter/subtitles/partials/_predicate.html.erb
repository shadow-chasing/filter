<% title controller_name + " " + action_name %>
<table>
    <thead>
        <tr>
            <th> word </th>
            <th> counter </th>
            <th> frequency </th>
            <th> syllable </th>
            <th> group </th>
            <th> predicate </th>
            
        </tr> 
    </thead>
    <tbody>

    <tbody>
    <% @predicates.each do |subtitle| %>
    <tr>
        <td class="sub-word"><%= subtitle.word %></td>
        <td><%= subtitle.counter %></td>
        <td><%= subtitle.frequency.strftime("%H"":""%M"":""%S")  %></td>
        <td><%= subtitle.syllable %></td>
        <td><% for result in subtitle.predicate_group_results %></td>
        <td><% result.group %></td>
        <td><% end %></td>

        <td><%= subtitle.predicate_group_results.first.predicate %></td>
    </tr>
    <% end %>
    </tbody>
</table>


<%= will_paginate @predicates, inner_window: 1, outer_window: 1 %>
